# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
VITE_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=15000

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_JWT_EXPIRY=24h

# External Services
REACT_APP_AI_SERVICE_URL=http://localhost:5001
REACT_APP_STORAGE_URL=http://localhost:5002

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_ANALYTICS=true

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Social Media
REACT_APP_FACEBOOK_APP_ID=facebook-app-id
REACT_APP_LINKEDIN_CLIENT_ID=linkedin-client-id

# Storage
REACT_APP_S3_BUCKET=your-bucket-name
REACT_APP_CLOUDFRONT_URL=your-cloudfront-url

# Development
REACT_APP_DEBUG_MODE=true
NODE_ENV=development
